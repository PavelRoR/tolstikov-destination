#main
	padding 0 0 50px
	background-color: #7B8B88
	background-image url(../img/backs/main-bg.jpg)
	background-repeat no-repeat
	background-position: center bottom
	background-size: cover
.main-container
	padding 40px
	background-color: rgba(#fff,.86)
	width 50%
	border-radius: 5px
	text-align: center
.pre
	font-size: 16px
	font-family: OSB
	margin-bottom: 20px
.main-title
	font-size: 44px
	font-family: RO
	text-transform: uppercase
	color #F0651B
	small
		display block
		text-transform: none
		font-family: OSB
		font-size: 18px
.main-subtitle
	color: #667B8B
	font-size: 25px
	font-family: RO
	margin 20px 0 40px
.main-info
	font-size: 20px
	span
		display: block
		font-family: RO
		text-transform: uppercase
		font-size: 24px
	&:last-of-type
		margin-bottom: 50px